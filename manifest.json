{
  "manifest_version": 2,
  "name": "Unravl",
  "version": "0.1.1",
  "description": "Suppress JavaScript translation of websites written in ANY of your preferred languages.",
  "author": "arnold",
  "homepage_url": "https://github.com/central182/unravl",
  "browser_specific_settings": {
    "gecko": {
      "id": "{9b62f54c-91fe-445e-8ff7-359e57c169ae}"
    }
  },
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": ["*://*/*"],
      "js": ["main.js"]
    }
  ]
}
